<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>CoffeePlace</display-name>
	<welcome-file-list>
		
		<welcome-file>welcome.jsp</welcome-file>
		
	</welcome-file-list>
	
	<servlet>
		<servlet-name>dispatcherServlet</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/getDataFromHtml</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/searchAirport</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/searchAirportByName</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/deleteByID</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/deleteByName</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/updateByID</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/updateByName</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/updateRunwayByID</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/getAll</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/editAirport/id</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/editRunway/id</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/updateAirport</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/saveData</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/saveTaxi</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/getAllRunways</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/getAllTaxi</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>dispatcherServlet</servlet-name>
	<url-pattern>/deleteRunwayByID/runwayID</url-pattern>
	</servlet-mapping>
	
	
</web-app>